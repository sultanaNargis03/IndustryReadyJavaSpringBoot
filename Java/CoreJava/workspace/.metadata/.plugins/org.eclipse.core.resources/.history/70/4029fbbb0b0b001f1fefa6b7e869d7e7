package practice.two;

class Alpha extends Thread
{
	Alpha(String tname)
	{
		super(tname);
	}
	@Override
	public void run() 
	{
		System.out.println("i am running...");
	}
}


public class Thread1 
{
	public static void main(String[] args) 
	{
		Alpha t1=new Alpha("AlphaThread");
		t1.start();
	}
}
