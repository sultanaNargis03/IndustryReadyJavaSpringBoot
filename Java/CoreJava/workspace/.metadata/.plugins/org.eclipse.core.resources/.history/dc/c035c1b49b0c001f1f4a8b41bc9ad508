package practice;

import java.util.ArrayList;
import java.util.List;

public class LaunchPredicate {

	public static void main(String[] args)
	{
		List list=new ArrayList();
		
		list.add(20);
		list.add(10);
		list.add(4);
		list.add(44);
		list.add(8);
		
		list.stream()
			.filter(i->i%5==0)
			.forEach(i->System.out.println(i));

	}

}
