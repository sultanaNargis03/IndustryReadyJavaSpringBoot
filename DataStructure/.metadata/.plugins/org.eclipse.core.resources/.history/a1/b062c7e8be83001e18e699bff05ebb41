package sorting;

public class LaunchSort1 {

	public static void main(String[] args) 
	{
		int arr[]= {5,2,9,8,3,7};
		
		System.out.println("before sorting");

		for(int num:arr)
			System.out.println(num+" ");
		
		System.out.println("After sorting");
		
		for(int num:arr)
			System.out.println(num+" ");
	}

}
