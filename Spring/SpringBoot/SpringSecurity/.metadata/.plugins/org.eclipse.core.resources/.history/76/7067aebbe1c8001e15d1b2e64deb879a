package com.telusko.springsecdemo.controller;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
public class HelloController 
{
	@GetMapping("/hello")
	public String greet()
	{
		return "hello world";
	}
	
	@GetMapping("/about")
	public String about()
	{
		return "Telusko";
	}
}
